{
  "name": "reown-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'server/**/*' -e ts,tsx --exec 'ts-node --swc --project tsconfig.server.json' server.ts",
    "build": "next build",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json server.ts",
    "lint": "next lint --fix && prettier --write .",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "clean": "find . -type d -name 'dist' -not -path './node_modules/*' -not -path './.next/*' -not -path './.vscode/*' -exec rm -rf {} +"
  },
